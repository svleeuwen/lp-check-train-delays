{
  "owner_email": "replytosander@gmail.com",
  "publication_api_version": "1.0",
  "delivery_type": "push",
  "name": "Trein vertraging",
  "description": "Stuurt een publicatie bij treinvertraging op jouw traject",
  "delivered_on": "als er vertraging vanaf je vertrekstation binnen de aangegeven tijdsperiode",
  "send_timezone_info": false,
  "send_delivery_count": false,
  "external_configuration": false,
  "config": {
    "fields": [
      {
        "type": "text",
        "name": "time_slot_begin",
        "label": "Tijd van (uu:mm)"
      },
      {
        "type": "text",
        "name": "time_slot_end",
        "label": "Tijd tot (uu:mm)"
      },
      {
        "type": "text",
        "name": "push_offset_minutes",
        "label": "Vanaf hoeveel minuten vóór de begintijd mag er gepushed worden?"
      },
      {
        "type": "select",
        "name": "from_station",
        "label": "Station van",
        "options": [
          ["Delft", "delft"],
          ["Rotterdam", "rotterdam"],
          ["Schiedam", "schiedam"]
        ]
      },
      {
        "type": "select",
        "name": "to_station",
        "label": "Station naar",
        "options": [
          ["Delft", "delft"],
          ["Rotterdam", "rotterdam"],
          ["Schiedam", "schiedam"]
        ]
      }
    ]
  }
}
