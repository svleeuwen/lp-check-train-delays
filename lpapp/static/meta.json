{
  "owner_email": "replytosander@gmail.com",
  "publication_api_version": "1.0",
  "delivery_type": "push",
  "name": "Train delays (dutch)",
  "description": "Notifies you of any train delays on your route. (Dutch railway only)",
  "delivered_on": "when there are train delays within the set time frame. (This could be more than once if delays change during the time frame)",
  "send_timezone_info": false,
  "send_delivery_count": false,
  "external_configuration": false,
  "config": {
    "fields": [
      {
        "type": "text",
        "name": "time_slot_begin",
        "label": "Tijd van (uu:mm)"
      },
      {
        "type": "text",
        "name": "time_slot_end",
        "label": "Tijd tot (uu:mm)"
      },
      {
        "type": "text",
        "name": "push_offset_minutes",
        "label": "Vanaf hoeveel minuten vóór de begintijd wil je een bericht krijgen over een eventuele vertraging?"
      },
      {
        "type": "select",
        "name": "from_station",
        "label": "Station van",
        "options": [
          ["Delft", "delft"],
          ["Rotterdam", "rotterdam"],
          ["Schiedam", "schiedam"]
        ]
      },
      {
        "type": "select",
        "name": "to_station",
        "label": "Station naar",
        "options": [
          ["Delft", "delft"],
          ["Rotterdam", "rotterdam"],
          ["Schiedam", "schiedam"]
        ]
      }
    ]
  }
}
